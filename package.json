{
  "name": "@loanlink/cxml",
  "version": "0.2.14",
  "description": "Advanced schema-aware streaming XML parser",
  "author": "Loanlink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Capital-Management-Group/cxml.git"
  },
  "bugs": {
    "url": "https://github.com/Capital-Management-Group/cxml/issues"
  },
  "homepage": "https://github.com/Capital-Management-Group/cxml#readme",
  "type": "module",
  "exports": {
    "typings": "./dist/cxml.d.ts",
    "default": "./dist/cxml.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "mkdirp dist && rimraf dist/",
    "build:pegjs": "mkdirp dist/spinoffs && pegjs -o dist/spinoffs/XPathParser.js src/spinoffs/XPathParser.pegjs",
    "build:typescript": "ncp src/spinoffs/XPathParser.d.ts dist/spinoffs/XPathParser.d.ts && tsc --project tsconfig.json",
    "format": "prettier --write ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm i --package-lock-only && npm run test",
    "postpublish": "TAG=\"v$(jq -r .version package.json)\" && git commit -am \"Bumped to version $TAG\" && git tag -a $TAG -m \"Published version $TAG\" && git push origin $TAG",
    "test": "npm-run-all test:* && rimraf ./test/**/*.js",
    "test:build": "npm-run-all test:build:*",
    "test:build:clean": "cd test && mkdirp cache && rimraf cache && mkdirp xmlns && rimraf xmlns && cd ..",
    "test:build:compile": "npm-run-all test:build:compile:*",
    "test:build:compile:dir-example": "cxsd \"./test/input/dir-example.xsd\"",
    "test:build:ts": "tsc --project test.tsconfig.json >/dev/null",
    "test:localize": "npm-run-all test:localize:*",
    "test:run": "jest"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "xml",
    "streaming",
    "schema",
    "parser",
    "xsd",
    "dts",
    "typescript"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "sax": "^1.2.4"
  },
  "devDependencies": {
    "@loanlink/cxsd": "../cxsd",
    "@types/jest": "^24.0.21",
    "@types/lodash": "^4.14.144",
    "@types/node": "^10.17.0",
    "@types/sax": "^1.2.0",
    "@types/source-map-support": "^0.5.6",
    "jest": "^29.1.0",
    "mkdirp": "^2.1.6",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pegjs": "^0.10.0",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "haste": {
      "providesModuleNodeModules": [
        ".*"
      ]
    },
    "testRegex": "((/src/.*\\.test\\.(ts|tsx|js))|(/test/spec/.*\\.(js|jsx)))$"
  },
  "jestts": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "transform": {
      "\\.(ts|tsx|.js|.jsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/(node_modules)/"
    ],
    "testRegex": "((/src/.*\\.test\\.(ts|tsx|js))|(/test/spec/.*\\.(ts|tsx|js)))$"
  }
}
