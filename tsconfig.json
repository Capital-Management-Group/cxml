{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["dom", "dom.iterable", "es5", "es6", "scripthost"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist",
    "removeComments": false,
    "target": "es5"
  },
  "include": ["src/**/*.ts"]
}
